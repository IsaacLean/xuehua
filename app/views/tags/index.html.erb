<h1>All Tags</h1>
<ul id="tags">
	<% @tags.each do |tag| %>
	<li>
		<%= link_to tag.name, tag_path(tag) %> |
		<%= link_to "delete", tag_path(tag), data: {confirm: "Are you sure you want to delete this tag?"}, method: :delete %>
	</li>
	<% end %>
	<br>
	<%= link_to "<< Back to Articles List", articles_path %>
</ul>